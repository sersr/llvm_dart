import "std:d.kc"

fn main() i32 {
  type BaseAlias<T> = Base<T>;

  let alias = BaseAlias{ 10, 22,  555};
  printf("type alias: %d\n", alias.z);

  type DoubleAlias  = BaseAlias<f32>;

  let doubleAlias = DoubleAlias {12, 22, };
  doubleAlias.z = 555.0;

  // f32 需要转换成 f64
  printf("double alias: %f\n", doubleAlias.z as f64);

  0;
}


struct Base<T> {
  y: i32,
  x: i32,
  z: T,
}