import '../lib/d.kc';

fn main() i32 {

    let arr:Array<i32> = Array.new(10);

    let i = 0;
    let max = arr.size as i32;

    printf('max: %d\n', max);

    loop {
        if i > max {
            printf('hello\n');
            break;
        }
        arr.elementAt(i as usize) = i + 1;
        i += 1;
    }

    i = 0;
    while i < max {
        printf('value: %d\n', arr.elementAt(i));
        i += 1;
    }
    0;
}

