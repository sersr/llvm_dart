extern fn printf(str: string, ...) i32;

fn print(str: string) {
    printf("log: %s\n", str);
}

struct Array<T>;

impl Array<T> {

    static fn new(size: usize) T;
    
    fn getSize() usize;

    fn elementAt(index: usize) &T;
}

impl Array<i8> {
  fn toStr() &string;
}

type Ptr<T>;
struct CArray<T> {
  size: usize,
  ptr: Ptr<T>,
}

impl CArray<T> {
  fn elementAt(index: usize) &T;
  fn getSize() usize {
    self.size
  }
}